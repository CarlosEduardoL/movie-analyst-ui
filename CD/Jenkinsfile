pipeline {
    agent {
        docker { 
            image 'zeronetdev/rampup-provcli:0.0.1'
            args '-v /home/ubuntu/RampUp/ansible/playbooks:/playbooks'
        }
    }
    stages {
        stage("deploy"){
            steps {
                sh "echo ${params.TAG}"
                sh "ls /playbooks"
            }
        }
    }
}